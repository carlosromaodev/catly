{
  "name": "catly",
  "version": "1.0.0",
  "description": "Catly API",
  "main": "index.js",
  "scripts": {
    "test": "vitest run --dir src/use-case",
    "test:watch": "vitest --dir src/use-case",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:preparar": "npm link ./prisma/vitest-environment-prisma",
    "test:executar": "npm link vitest-environment-prisma",
    "pretest:e2e": " run-s test:preparar test:executar",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "format": "biome format",
    "dev": "tsx watch src/server.ts ",
    "build": "tsup src --out-dir build",
    "start": "node build/server.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.9.1",
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "^22.13.4",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "2.0.5",
    "@vitest/ui": "2.0.5",
    "npm-run-all": "4.1.5",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "prisma": "5.17.0",
    "supertest": "7.0.0",
    "tsup": "^8.2.3",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.0.5"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.1",
    "@fastify/jwt": "9.0.1",
    "@fastify/swagger": "^9.4.2",
    "@prisma/client": "5.17.0",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.13",
    "dotenv": "16.4.5",
    "fastify": "5.1.0",
    "resend": "^4.1.2",
    "run-all": "1.0.1",
    "zod": "3.23.8"
  }
}
